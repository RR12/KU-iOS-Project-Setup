project 'iOS Project Setup.xcodeproj'

# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'iOS Project Setup' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for iOS Project Setup
  pod 'RxSwift', '~> 4.0'
  pod 'RxCocoa', '~> 4.0'
  pod 'Swinject', '~> 2.1.0'
  pod 'Alamofire', '~> 4.4'
  pod 'Moya'
  pod 'Moya/RxSwift'
  pod 'Gloss', '2.0.0-beta.2'
  pod 'Moya-Gloss'
  pod 'Moya-Gloss/RxSwift'
  pod 'RealmSwift'
  pod 'IGListKit'
  pod 'Kingfisher'

end
